    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="myLogic" class="orz.an.study.spring.di.MyLogic">
        <property name="service" ref="chService"/>
    </bean>

    <bean id="chService" class="orz.an.study.spring.di.ChService"/>

    <bean id="enService" class="orz.an.study.spring.di.EnService"/>

</beans>